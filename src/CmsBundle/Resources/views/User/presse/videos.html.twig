<div class="container-fluid">
    <div class="title_section">
        <h2>Videos</h2>
    </div>
    <div class="row">
        {% for video in videos %}
            <div class="col-md-3 row_video">
                <h5>{{ video.title }}</h5>
                {{ video.link|raw }}
            </div>
        {% endfor %}
    </div>
</div>